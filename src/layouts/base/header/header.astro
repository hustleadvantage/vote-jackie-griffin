---
import { config } from '../../../constants';
import AnnouncementBar from './announcement-bar.astro';
import Navbar from './navbar.astro';

const announcement = {
  message: 'Voting Information',
  action: {
    text: 'Learn More',
    url: 'https://www2.monroecounty.gov/etc/voter/',
  },
};

const { links, actions } = config.navigation;
---

<header class='bg-white border-b border-b-neutral-100'>
  <AnnouncementBar {announcement} />
  <Navbar {links} {actions} />
</header>
